{
  "name": "zcalendar-cds",
  "version": "1.0.0",
  "description": "Generated by cds-services-archetype",
  "license": "ISC",
  "repository": "<Add your repository here>",
  "scripts": {
    "start": "cds run",
    "build": "cds build --clean",
    "sqlite": "cds deploy --to sqlite:./db/sqlite.db",
    "schema": "cds compile srv --to sql > srv/src/main/resources/schema.sql"
  },
  "cds": {
    "build": {
      "target": ".",
      "tasks": [
        {
          "for": "java-cf",
          "src": "srv",
          "options": {
            "model": [
              "db",
              "srv"
            ]
          }
        }
      ]
    },
    "requires": {
      "db": {
        "kind": "sqlite",
        "model": "*",
        "credentials": {
          "database": "./db/sqlite.db"
        }
      }
    },
    "features": {
      "format": "flat",
      "version": "v4",
      "snapi": true
    }
  },
  "dependencies": {
    "@sap/cds": "^4.4.9",
    "@sap/cds-dk": "^3.3.4"
  }
}
